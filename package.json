{
  "name": "@auroratide/sifetti",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json --fail-on-warnings --fail-on-hints",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "uvu -r dotenv/config -r tsm -r module-alias/register test",
    "test:all": "npm run check && uvu -r dotenv/config -r tsm -r module-alias/register",
    "migrate": "node -r dotenv/config db/migrate.js",
    "ci": "npm-run-all check build"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-netlify": "^1.0.0-next.48",
    "@sveltejs/kit": "^1.0.0-next.287",
    "@types/cookie": "^0.4.1",
    "@types/dompurify": "^2.3.2",
    "@types/jsdom": "^16.2.14",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/marked": "^4.0.1",
    "@types/rollup-plugin-css-only": "^3.1.0",
    "@types/supertest": "^2.0.11",
    "chromium": "^3.0.3",
    "dotenv": "^10.0.0",
    "jsdom": "^19.0.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "node-fetch": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "postgres-migrations": "^5.3.0",
    "rollup": "^2.63.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "sass": "^1.45.2",
    "supertest": "^6.1.6",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-preprocess": "^4.10.4",
    "taiko": "^1.2.8",
    "tslib": "^2.3.1",
    "tsm": "^2.2.1",
    "typescript": "^4.4.3",
    "uvu": "^0.5.2"
  },
  "type": "module",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^1.29.0",
    "cookie": "^0.4.1",
    "dompurify": "^2.3.4",
    "fp-ts": "^2.11.8",
    "io-ts": "^2.2.16",
    "marked": "^4.0.9"
  },
  "_moduleAliases": {
    "$lib": "src/lib",
    "$test": "test"
  }
}
